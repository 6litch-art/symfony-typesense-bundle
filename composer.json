{
    "name": "glitchr/typesense-bundle",
    "description": "This bundle provides integration with Typesense in Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Marco Meyer",
            "email": "marco.meyer@glitchr.io"
        }
    ],
    "require": {
        "php": "^7.4||^8.0",
        "doctrine/orm": "^2.0",
        "doctrine/doctrine-bundle": "^2.0",
        "symfony/framework-bundle": "^3.4|^4.3|^5|^6.0",
        "symfony/http-client-contracts": "^1.0|^2.0|^3.0",
        "typesense/typesense-php": "^4.0",
        "php-http/curl-client": "^2.2",

        "monolog/monolog": "^2.3",
        "symfony/property-access": "^3.4|^4.3|^5|^6.0",
        "beberlei/doctrineextensions": "^1.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0|^6.0",
        "phpunit/phpunit": "^9.5",
        "symfony/yaml": "^3.4 || ^4.4 || ^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": { "Typesense\\Bundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Typesense\\Bundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
    }
}
